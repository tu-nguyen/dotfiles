# ~/.config/dotfiles/.bash_tt

br="${BOLD}${H1}----------------------------------------------------------------------------------${NC}"

# Generalized function printer
function print_tt() {
  local file="$1"
  if [[ ! -f "$file" ]]; then
    echo "File not found: $file"
    return 1
  fi

  awk -v h="$H" -v nc="$NC" '
  BEGIN { comment="" }
  /^#/ { comment = $0; next }
  /^[a-zA-Z_][a-zA-Z0-9_]* *\(\) *\{/ {
      gsub(/^# *|^## */, "", comment)
      sub(/ *\(\) *\{/, "", $0)
      printf "  %s%-20s%s → %s\n", h, $0, nc, comment
      comment=""
  }' "$file"
}

function tt() {
  if [ -z "$DOTFILES_CONFIG_DIR" ]; then
    DOTFILES_CONFIG_DIR="$HOME/.config/dotfiles"
    printf "${BOLD}${RED}[WARNING]${NC} DOTFILES_CONFIG_DIR is not set, defaulting to $DOTFILES_CONFIG_DIR"
  fi

  printf "${BOLD}${H1}tt cheatsheet${NC}\n"
  printf "%s\n" "$br"
  # TODO: add chmod aliases
  printf "${H2}primary functions${NC}      → ${H}ttt${NC} (show all tt)            → ${H}j${NC} (jobs -l)\n"
  printf "                       → ${H}cls${NC} (clear and ls)           → ${H}df${NC} (df -H)\n"
  printf "                       → ${H}mkcd${NC} (mkdir then cd)         → ${H}du${NC} (du -ch)\n"
  printf "                       → ${H}h${NC} (history)                  → ${H}dmesg10${NC} (10 recent logs)\n"
  printf "                       → ${H}x${NC} (exit)                     → ${H}top${NC} (htop)\n"
  printf "                       → ${H}v${NC} (vim)                      → ${H}top10${NC} (top 10 ps)\n"
  # ls
  printf "%s\n" "$br"
  printf "${H2}ls & cd${NC}                → ${NC}ll${NC} (ls -alh)                 → ${H}~${NC} (cd ~)\n"
  printf "                       → ${H}la${NC} (ls -A)                   → ${H}..${NC} (cd ..)\n"
  printf "                       → ${H}lsd${NC} (ls -a1F | grep /$)      → ${H}...${NC} (cd ../..)\n"
  printf "                       → ${H}lt${NC} (la -lt)                  → ${H}....${NC} (cd ../../..)\n"
  printf "                       → ${H}la${NC} (ls -A)                   → ${H}.4${NC} (../../../../)\n"
  # cd
  printf "%s\n" "$br"
  printf "${H2}utils${NC}                  → ${H}myip${NC} (get ip)                → ${H}now${NC}/${H}nowtime${NC} (time)\n"
  printf "                       → ${H}ping${NC} (ping -c 5)             → ${H}nowdate${NC} (date)\n"
  printf "                       → ${H}p4${NC} (ping 4.2.2.2 -c 4)       → ${H}ipt${NC} (sudo /sbin/iptables)\n"
  printf "                       → ${H}ports${NC} (netstat -tulanp)      → ${H}diskspace${NC} (du)\n"
  printf "                       → ${H}folder${NC} ()                    → ${H}path${NC} ()→\n"
  # other
  printf "%s\n" "$br"
  printf "${H2}other functions${NC}      \n"
  print_tt "$DOTFILES_CONFIG_DIR/.bash_functions"
  # other other
  printf "%s\n" "$br"
  printf "${H2}other tt${NC}      \n"
  printf "  ${H}btt${NC}                  → to view bash cheatsheet\n"
  printf "  ${H}djtt${NC}                 → to view django cheatsheet\n"
  printf "  ${H}gtt${NC}                  → to view git aliases\n"
  printf "  ${H}makett${NC}               → to view makefile cheatsheet\n"
  printf "  ${H}pytt${NC}                 → to view python cheatsheet\n"
  printf "  ${H}dctt${NC}                 → to view docker-compose cheatsheet\n"
  printf "  ${H}dktt${NC}                 → to view docker stuff\n"
  printf "  ${H}vtt${NC}                  → to view vim cheatsheet\n"

  if [[ ! -f "$DOTFILES_CONFIG_DIR/.bash_personal" ]]; then
    printf "  ${H}ptt${NC}                  → .bash_personal ${RED}does not exist${NC} inside $DOTFILES_CONFIG_DIR\n"
  else
    printf "  ${H}ptt${NC}                  → to view personal stuff\n"
  fi

  if [[ ! -f "$DOTFILES_CONFIG_DIR/.bash_twork" ]]; then
    printf "  ${H}tworktt${NC}              → .bash_twork ${RED}does not exist${NC} inside $DOTFILES_CONFIG_DIR\n"
    printf "                             run ${H}set-tworktt${NC} to copy .bash_twork template\n"
  else
    printf "  ${H}tworktt${NC}              → to view twork stuff\n"
  fi

  printf "%s\n" "$br"
  printf "${H2}reminder${NC}      \n"
  printf "  ${H}cd-dotfiles${NC}          → to acd to original dotfiles dir\n"
}

function btt() {
  printf "${BOLD}${H1}bash cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}navigation:${NC}\n"
  printf "  ${H}cd${NC} ${S}dir${NC}               → Change directory\n"
  printf "  ${H}cd -${NC}                 → Previous directory\n"
  printf "  ${H}pwd${NC}                  → Print working directory\n"
  printf "  ${H}ls -al${NC}               → List files\n"
  printf "%s\n" "$br"
  printf "${H2}file ops:${NC}\n"
  printf "  ${H}cp${NC} ${S}src dst${NC}           → Copy file\n"
  printf "  ${H}mv${NC} ${S}src dst${NC}           → Move/rename file\n"
  printf "  ${H}rm${NC} ${S}file${NC}              → Remove file\n"
  printf "  ${H}mkdir${NC} ${S}dir${NC}            → Make directory\n"
  printf "  ${H}touch${NC} ${S}file${NC}           → Create empty file\n"
  printf "%s\n" "$br"
  printf "${H2}search:${NC}\n"
  printf "  ${H}grep${NC} ${S}PATTERN file${NC}    → Search in file\n"
  printf "  ${H}tail -f${NC} ${S}file${NC}        → Follow file changes\n"
  printf "  ${H}find . -name${NC} ${S}'*.sh'${NC}  → Find files\n"
  printf "%s\n" "$br"
  printf "${H2}process:${NC}\n"
  printf "  ${H}ps aux${NC}               → List processes (ps -a | grep firefox)\n"
  printf "  ${H}top${NC}                  → Interactive process viewer\n"
  printf "  ${H}kill${NC} ${S}PID${NC}             → Kill process\n"
  printf "%s\n" "$br"
  printf "${H2}history & jobs:${NC}\n"
  printf "  ${H}history${NC}              → Show command history\n"
  printf "  ${H}!!${NC}                   → Repeat last command\n"
  printf "  ${H}!n${NC}                   → Repeat command n\n"
  printf "  ${H}jobs${NC}                 → List background jobs\n"
  printf "  ${H}fg/bg${NC}                → Foreground/background job\n"
  printf "%s\n" "$br"
  printf "${H2}shortcuts:${NC}\n"
  printf "  ${H}Ctrl+a${NC}               → Start of line\n"
  printf "  ${H}Ctrl+e${NC}               → End of line\n"
  printf "  ${H}Ctrl+r${NC}               → Reverse search history\n"
  printf "  ${H}Ctrl+c${NC}               → Cancel command\n"
  printf "  ${H}Ctrl+d${NC}               → Logout/EOF\n"
  printf "%s\n" "$br"
  printf "${H2}scripting:${NC}\n"
  printf "  ${H}echo${NC} ${S}\"text\"${NC}          → Print text\n"
  printf "  ${S}VAR${NC}${H}=${NC}${S}value${NC}            → Set variable\n"
  printf "  ${S}\$VAR${NC}                 → Use variable\n"
  printf "  ${H}for i in *; do ... done${NC}\n"
  printf "  ${H}if ...; then ...; fi${NC}\n"
  printf "  ${H}function fname() { ... }${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}other:${NC}\n"
  printf "  ${H}curl${NC}                 → Check connectivity (ping 8.8.8.8) Network troubleshooting 101\n"
  printf "  ${H}ping${NC}                 → Grab data(curl -O http://example.com/file.txt) Downloads without a browser\n"
  printf "%s\n" "$br"
}

