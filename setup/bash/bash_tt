# ~/.bash_extras/.bash_tt

br="${BOLD}${H1}----------------------------------------------------------------------------------${NC}"

# Generalized function printer
function print_tt() {
    local file="$1"
    if [[ ! -f "$file" ]]; then
        echo "File not found: $file"
        return 1
    fi

    awk '
    BEGIN { comment="" }
    /^#/ { comment = $0; next }
    /^[a-zA-Z_][a-zA-Z0-9_]* *\(\) *\{/ {
        gsub(/^# *|^## */, "", comment)
        sub(/ *\(\) *\{/, "", $0)
        printf "  %-20s → %s\n", $0, comment
        comment=""
    }' "$file"
}

function tt() {
  printf "${BOLD}${H1}tt cheatsheet${NC}\n"
  printf "%s\n" "$br"
  # TODO: add chmod aliases
  printf "${H2}primary functions${NC}      → ${H}ttt${NC} (show all tt)            → ${H}j${NC} (jobs -l)\n"
  printf "                       → ${H}cls${NC} (clear and ls)           → ${H}df${NC} (df -H)\n"
  printf "                       → ${H}mkcd${NC} (mkdir then cd)         → ${H}du${NC} (du -ch)\n"
  printf "                       → ${H}h${NC} (history)                  → ${H}dmesg10${NC} (10 recent logs)\n"
  printf "                       → ${H}x${NC} (exit)                     → ${H}top${NC} (htop)\n"
  printf "                       → ${H}v${NC} (vim)                      → ${H}top10${NC} (top 10 ps)\n"
  # ls
  printf "%s\n" "$br"
  printf "${H2}ls & cd${NC}                → ${NC}ll${NC} (ls -alh)                 → ${NC}~${NC} (cd ~)\n"
  printf "                       → ${NC}la${NC} (ls -A)                   → ${NC}..${NC} (cd ..)\n"
  printf "                       → ${NC}lsd${NC} (ls -a1F | grep /$)      → ${NC}...${NC} (cd ../..)\n"
  printf "                       → ${NC}lt${NC} (la -lt)                  → ${NC}....${NC} (cd ../../..)\n"
  printf "                       → ${NC}la${NC} (ls -A)                   → ${NC}.4${NC} (../../../../)\n"
  # cd
  printf "%s\n" "$br"
  printf "${H2}utils${NC}                  → ${NC}myip${NC} (get ip)                → ${NC}now${NC}/${NC}nowtime${NC} (time)\n"
  printf "                       → ${NC}ping${NC} (ping -c 5)             → ${NC}nowdate${NC} (date)\n"
  printf "                       → ${NC}p4${NC} (ping 4.2.2.2 -c 4)       → ${NC}ipt${NC} (sudo /sbin/iptables)\n"
  printf "                       → ${NC}ports${NC} (netstat -tulanp)      → ${NC}diskspace${NC} (du)\n"
  printf "                       → ${NC}folder${NC} ()                    → ${NC}path${NC} ()→\n"
  # other
  printf "%s\n" "$br"
  printf "${H2}other functions${NC}      \n"
  print_tt "$HOME/.bash_extras/.bash_functions"
  # other other
  printf "%s\n" "$br"
  printf "${H2}other tt${NC}      \n"
  printf "  ${NC}btt${NC}                  → to view bash cheatsheet\n"
  printf "  ${NC}djtt${NC}                 → to view django cheatsheet\n"
  printf "  ${NC}gtt${NC}                  → to view git aliases\n"
  printf "  ${NC}makett${NC}               → to view makefile cheatsheet\n"
  printf "  ${NC}pytt${NC}                 → to view python cheatsheet\n"
  printf "  ${NC}dctt${NC}                 → to view docker-compose cheatsheet\n"
  printf "  ${NC}dktt${NC}                 → to view docker stuff\n"
  printf "  ${NC}vtt${NC}                  → to view vim cheatsheet\n"

  if [[ ! -f "$HOME/.bash_extras/.bash_personal" ]]; then
    printf "  ${NC}ptt${NC}                  → .bash_personal ${RED}does not exist${NC} inside $HOME/.bash_extras\n"
  else
    printf "  ${NC}ptt${NC}                  → to view personal stuff\n"
  fi

  if [[ ! -f "$HOME/.bash_extras/.bash_twork" ]]; then
    printf "  ${NC}tworktt${NC}              → .bash_twork ${RED}does not exist${NC} inside $HOME/.bash_extras\n"
  else
    printf "  ${NC}tworktt${NC}              → to view twork stuff\n"
  fi

  printf "%s\n" "$br"
}

function btt() {
  printf "${BOLD}${H1}bash cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}navigation:${NC}\n"
  printf "  ${NC}cd${NC} ${NC}dir${NC}               → Change directory\n"
  printf "  ${NC}cd -${NC}                 → Previous directory\n"
  printf "  ${NC}pwd${NC}                  → Print working directory\n"
  printf "  ${NC}ls -al${NC}               → List files\n"
  printf "%s\n" "$br"
  printf "${H2}file ops:${NC}\n"
  printf "  ${NC}cp${NC} ${NC}src dst${NC}           → Copy file\n"
  printf "  ${NC}mv${NC} ${NC}src dst${NC}           → Move/rename file\n"
  printf "  ${NC}rm${NC} file              → Remove file\n"
  printf "  ${NC}mkdir${NC} ${NC}dir${NC}            → Make directory\n"
  printf "  ${NC}touch${NC} ${NC}file${NC}           → Create empty file\n"
  printf "%s\n" "$br"
  printf "${H2}search:${NC}\n"
  printf "  ${NC}grep${NC} ${NC}PATTERN file${NC}    → Search in file\n"
  printf "  ${NC}tail -f${NC} ${NC}file${NC}        → Follow file changes\n"
  printf "  ${NC}find . -name${NC} ${NC}'*.sh'${NC}  → Find files\n"
  printf "%s\n" "$br"
  printf "${H2}process:${NC}\n"
  printf "  ${NC}ps aux${NC}               → List processes\n"
  printf "  ${NC}top${NC}                  → Interactive process viewer\n"
  printf "  ${NC}kill${NC} ${NC}PID${NC}             → Kill process\n"
  printf "%s\n" "$br"
  printf "${H2}history & jobs:${NC}\n"
  printf "  ${NC}history${NC}              → Show command history\n"
  printf "  ${NC}!!${NC}                   → Repeat last command\n"
  printf "  ${NC}!n${NC}                   → Repeat command n\n"
  printf "  ${NC}jobs${NC}                 → List background jobs\n"
  printf "  ${NC}fg/bg${NC}                → Foreground/background job\n"
  printf "%s\n" "$br"
  printf "${H2}shortcuts:${NC}\n"
  printf "  Ctrl+a               → Start of line\n"
  printf "  Ctrl+e               → End of line\n"
  printf "  Ctrl+r               → Reverse search history\n"
  printf "  Ctrl+c               → Cancel command\n"
  printf "  Ctrl+d               → Logout/EOF\n"
  printf "%s\n" "$br"
  printf "${H2}scripting:${NC}\n"
  printf "  ${NC}echo${NC} \"text\"          → Print text\n"
  printf "  ${NC}VAR${NC}${NC}=${NC}${NC}value${NC}            → Set variable\n"
  printf "  ${NC}\$VAR${NC}                 → Use variable\n"
  printf "  ${NC}for i in *; do ... done${NC}\n"
  printf "  ${NC}if ...; then ...; fi${NC}\n"
  printf "  ${NC}function fname() { ... }${NC}\n"
  printf "%s\n" "$br"
}

