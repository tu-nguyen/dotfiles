function gtt() {
  printf "${BOLD}${H1}git cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}git${NC}                    → ${H}ga${NC} (add)                     → ${H}gb${NC} (branch)\n"
  printf "                       → ${H}gs${NC} (status)                  → ${H}gbr${NC} (fance branch)\n"
  printf "                       → ${H}gc${NC} (cam)                     → ${H}gbd${NC} (delete branch)\n"
  printf "                       → ${H}cl${NC} (log)                     → ${H}gcl${NC} (cl show last commit)\n"
  printf "                       → ${H}crr${NC} (hard reset)             → ${H}gll${NC} (bigger log)\n"
  printf "                       → ${H}gf${NC} (git fetch)               → ${H}gfetch${NC} (also git fetch)\n"
  printf "${H2}git alt${NC}                → ${NC}s${NC} (status -s)                → ${H}b${NC} (fancy branch)\n"
  printf "                       → ${H}st${NC} (status)                  → ${H}br${NC} (branch)\n"
  printf "                       → ${H}co${NC} (checkout)                → ${H}a${NC} (add)\n"
  printf "                       → ${H}cob${NC} (checkout -b)            → ${H}ap${NC} (add -p)\ \n"
  printf "                       → ${H}coB${NC} (checkout -B)            → ${H}l${NC} (diff)\n"
  printf "                       → ${H}c${NC} (commit)                   → ${NC}d${NC} (diff)\n"
  printf "                       → ${H}ca${NC} (commit -a)               → ${NC}ds${NC} (diff --stat)\n"
  printf "                       → ${H}cm${NC} (commit -m)               → ${NC}dc${NC} (diff --cached)\n"
  printf "                       → ${H}cam${NC} (commit -a -m)           \n"
  printf "                       → ${H}m${NC} (commit -m)                → ${H}la${NC} (to ALL git config)\n"
  printf "%s\n" "$br"
  printf "${H2}git functions${NC}      \n"
  printf "  ${H}gpull${NC}                → Git pull with rebase and autostash\n"
  printf "  ${H}gpush${NC}                → Git push with set-upstream\n"
  printf "  ${H}gco${NC}                  → Switch to a specified Git branch with confirmation\n"
  printf "  ${H}gcob${NC}                 → Same as above but with fallback to create a branch\n"
  printf "  ${H}greset${NC}               → Hard reset on current branch\n"
  printf "  ${H}gclean${NC}               → Hard reset and removes untracked files\n"
  printf "  ${H}gclone${NC}               → Clone a repo\n"
  printf "  ${H}grn${NC}                  → Rename git branch\n"
  printf "%s\n" "$br"

  printf "${H2}ghcli run${NC}\n"
  printf "  ${H}gh run list${NC}          → List runners (all details)\n"
  printf "  ${H}gh run cancel${NC} ${S}id${NC}     → Cancel a workflow run\n"
  printf "  ${H}gh run list --json databaseId -q '.[].databaseId' | xargs -d '\\\\n' -n1 gh run cancel${NC}\n"
  printf "                       → Cancels all runners\n"
  printf "  ${H}gh run delete${NC} ${S}id${NC}     → Delete a workflow run\n"
  printf "  ${H}gh run rerun${NC} ${S}id${NC}      → Rerun a workflow run\n"
  printf "  ${H}gh run view${NC} ${S}id${NC}       → View a workflow run\n"
  printf "  ${H}gh run watch${NC} ${S}id${NC}      → Watch a workflow run until it completes\n"
  printf "%s\n" "$br"
  printf "${H2}todo:${NC}\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "%s\n" "$br"
  printf "${H2}todo:${NC}\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "%s\n" "$br"
  printf "${H2}todo:${NC}\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "%s\n" "$br"
}


function makett() {
  printf "${BOLD}${H1}makefile cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}common targets:${NC}\n"
  printf "  ${H}make${NC}                → Run default target\n"
  printf "  ${H}make${NC} ${S}target${NC}     → Run specific target\n"
  printf "%s\n" "$br"
  printf "${H2}variables:${NC}\n"
  printf "  ${S}VAR${NC} ${H}=${NC} ${S}value${NC}         → Set variable\n"
  printf "  ${S}\$(VAR)${NC}              → Use variable\n"
  printf "%s\n" "$br"
  printf "${H2}phony targets:${NC}\n"
  printf "  ${S}.PHONY: clean${NC}       → Declare phony target\n"
  printf "%s\n" "$br"
  printf "${H2}examples:${NC}\n"
  printf "  ${S}.PHONY: clean\n"
  printf "  clean:\n"
  printf "    rm -rf *.pyc __pycache__/${NC}\n"
  printf "%s\n" "$br"
}

function pytt() {
  printf "${BOLD}${H1}python cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}run & env:${NC}\n"
  printf "  ${H}python${NC} ${S}file.py${NC}            → Run script\n"
  printf "  ${H}python -m venv${NC} ${S}venv${NC}       → Create virtualenv\n"
  printf "  ${H}source venv/bin/activate${NC}  → Activate venv\n"
  printf "  ${H}pip install${NC} ${S}package${NC}       → Install package\n"
  printf "  ${H}pip freeze >${NC} ${S}req.txt${NC}      → Export requirements\n"
  printf "  ${H}pip install -r${NC} ${S}req.txt${NC}    → Install from requirements\n"
  printf "%s\n" "$br"
  printf "${H2}common commands:${NC}\n"
  printf "  ${H}python -m http.server${NC}     → Simple HTTP server\n"
  printf "  ${H}python -i${NC} ${S}file.py${NC}         → Run and enter REPL\n"
  printf "  ${H}python -c${NC} ${S}'code'${NC}          → Run one-liner\n"
  printf "%s\n" "$br"
  printf "${H2}testing:${NC}\n"
  printf "  ${H}pytest${NC}                    → Run tests\n"
  printf "  ${H}pytest -k${NC} ${S}test_func${NC}       → Run specific test\n"
  printf "  ${H}coverage run -m pytest${NC}    → Run tests with coverage\n"
  printf "  ${H}coverage report${NC}           → Show coverage\n"
  printf "%s\n" "$br"
  printf "${H2}formatting & lint:${NC}\n"
  printf "  ${H}black .${NC}                   → Format code\n"
  printf "  ${H}isort .${NC}                   → Sort imports\n"
  printf "  ${H}flake8 .${NC}                  → Lint code\n"
  printf "%s\n" "$br"
}

function dctt() {
  printf "${BOLD}${H1}docker-compose cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}common commands:${NC}\n"
  printf "  ${H}docker-compose up -d${NC}                                → Start services\n"
  printf "  ${H}docker-compose down${NC}                                 → Stop and remove\n"
  printf "  ${H}docker-compose build${NC}                                → Build images\n"
  printf "  ${H}docker-compose logs${NC}                                 → Show logs\n"
  printf "  ${H}docker-compose exec svc sh${NC}                          → Shell in container\n"
  printf "%s\n" "$br"
  printf "${H2}other:${NC}\n"
  printf "  ${H}docker-compose ps${NC}                                   → List containers\n"
  printf "  ${H}docker-compose restart svc${NC}                          → Restart service\n"
  printf "%s\n" "$br"
}

function dktt() {
  printf "${BOLD}${H1}docker cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}docker${NC}                 → ${H}dk${NC} (docker)\n"
  printf "                       → ${H}dkps${NC} (list running containers)\n"
  printf "                       → ${H}dkc${NC} (list all docker containers)\n"
  printf "                       → ${H}dki${NC} (list docker images)\n"
  printf "                       → ${H}dkdf${NC} (run docker system df)\n"
  printf "                       → ${H}dkl${NC} (run docker logs <requires at least 1 argument CONTAINER>)\n"
  printf "                       → ${H}dklf${NC} (docker logs -f <requires at least 1 argument CONTAINER>)\n"
  printf "                       → ${H}dks${NC} (docker service <require COMMAND ie create, inspect, logs etc>)\n"
  printf "                       → ${H}dkrm${NC} (docker rm <requires at least 1 argument CONTAINER>)\n"
  printf "%s\n" "$br"
  printf "${H2}docker functions${NC}      \n"
  print_tt "$DOTFILES_CONFIG_DIR/.bash_docker_functions"
  printf "%s\n" "$br"
}

function vtt() {
  printf "${BOLD}${H1}vim cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}modes:${NC}\n"
  printf "  ${S}i${NC}                    → Insert mode\n"
  printf "  ${S}Esc${NC}                  → Normal mode\n"
  printf "  ${S}:${NC}                    → Command mode\n"
  printf "%s\n" "$br"
  printf "${H2}navigation:${NC}\n"
  printf "  ${S}h j k l${NC}              → Left Down Up Right\n"
  printf "  ${S}w/b/e${NC}                → Next/Prev/End of word\n"
  printf "  ${S}0/$ ${NC}                 → Start/End of line\n"
  printf "  ${S}gg/G${NC}                 → Top/Bottom of file\n"
  printf "%s\n" "$br"
  printf "${H2}editing:${NC}\n"
  printf "  ${S}x${NC}                    → Delete char\n"
  printf "  ${S}dd${NC}                   → Delete line\n"
  printf "  ${S}yy${NC}                   → Yank (copy) line\n"
  printf "  ${S}p${NC}                    → Paste\n"
  printf "  ${S}u${NC}                    → Undo\n"
  printf "  ${S}Ctrl+r${NC}               → Redo\n"
  printf "  ${S}r${NC}${WHITE}char${NC}                → Replace char\n"
  printf "%s\n" "$br"
  printf "${H2}search:${NC}\n"
  printf "  ${S}/${NC}${WHITE}pattern${NC}             → Search forward\n"
  printf "  ${S}?${NC}${WHITE}pattern${NC}             → Search backward\n"
  printf "  ${S}n/N${NC}                  → Next/Prev match\n"
  printf "%s\n" "$br"
  printf "${H2}save & quit:${NC}\n"
  printf "  ${S}:${NC}${WHITE}w${NC}                   → Save\n"
  printf "  ${S}:${NC}${WHITE}q${NC}                   → Quit\n"
  printf "  ${S}:${NC}${WHITE}wq${NC}                  → Save & Quit\n"
  printf "  ${S}:${NC}${WHITE}q!${NC}                  → Quit without saving\n"
  printf "%s\n" "$br"
}

function ttt() {
  printf "${BOLD}${H1}ttt: Command Reference${NC}\n"
  printf "%s\n" "$br"
  printf "  ${H2}%-20s %-30s %s${NC}\n" "Command" "Description" "Usage Example"
  printf "%s\n" "$br"
  printf "  %-20s %-30s %s\n" "c" "Clear the terminal" "c"
  printf "  %-20s %-30s %s\n" "cls" "Clear and list files" "cls"
  printf "  %-20s %-30s %s\n" "clss" "Clear and lsa" "clss"
  printf "  %-20s %-30s %s\n" "h" "Show command history" "h"
  printf "  %-20s %-30s %s\n" "x" "Exit the shell" "x"
  printf "  %-20s %-30s %s\n" "v" "Open vim editor" "v"
  printf "  %-20s %-30s %s\n" "j" "List background jobs" "j"
  printf "  %-20s %-30s %s\n" "df" "Show disk free space" "df -H"
  printf "  %-20s %-30s %s\n" "du" "Show disk usage" "du -ch"
  printf "  %-20s %-30s %s\n" "top" "Interactive process viewer" "top"
  printf "  %-20s %-30s %s\n" "top10" "Show top 10 CPU processes" "top10"
  printf "  %-20s %-30s %s\n" "dmesg10" "Last 10 kernel messages" "dmesg10"
  printf "  %-20s %-30s %s\n" "uptime" "Show system uptime" "uptime -p"
  printf "  %-20s %-30s %s\n" "mkcd" "Make and cd into directory" "mkcd mydir"
  printf "  %-20s %-30s %s\n" "rmd" "Remove directory recursively" "rmd mydir"
  printf "  %-20s %-30s %s\n" "mx" "Make file executable" "mx script.sh"
  printf "  %-20s %-30s %s\n" "000" "chmod 000 recursively" "000 mydir"
  printf "  %-20s %-30s %s\n" "644" "chmod 644 recursively" "644 mydir"
  printf "  %-20s %-30s %s\n" "666" "chmod 666 recursively" "666 mydir"
  printf "  %-20s %-30s %s\n" "755" "chmod 755 recursively" "755 mydir"
  printf "  %-20s %-30s %s\n" "777" "chmod 777 recursively" "777 mydir"
  printf "  %-20s %-30s %s\n" "ll" "List all files (long, human)" "ll"
  printf "  %-20s %-30s %s\n" "la" "List all files (almost all)" "la"
  printf "  %-20s %-30s %s\n" "l" "List files" "l"
  printf "  %-20s %-30s %s\n" "lsa" "List all files (long)" "lsa"
  printf "  %-20s %-30s %s\n" "lsd" "List directories only" "lsd"
  printf "  %-20s %-30s %s\n" "lt" "List files sorted by time" "lt"
  printf "  %-20s %-30s %s\n" "l." "List dotfiles" "l."
  printf "  %-20s %-30s %s\n" "home" "Go to home directory" "home"
  printf "  %-20s %-30s %s\n" "~" "Go to home directory" "~"
  printf "  %-20s %-30s %s\n" ".." "Go up one directory" ".."
  printf "  %-20s %-30s %s\n" "..." "Go up two directories" "..."
  printf "  %-20s %-30s %s\n" "...." "Go up three directories" "...."
  printf "  %-20s %-30s %s\n" ".4" "Go up four directories" ".4"
  printf "  %-20s %-30s %s\n" ".5" "Go up five directories" ".5"
  printf "  %-20s %-30s %s\n" "ga" "Git add" "ga file"
  printf "  %-20s %-30s %s\n" "gb" "Git branch" "gb"
  printf "  %-20s %-30s %s\n" "gbr" "Git fancy branch" "gbr"
  printf "  %-20s %-30s %s\n" "gc" "Git commit all message" "gc 'msg'"
  printf "  %-20s %-30s %s\n" "gcl" "Git show last commit" "gcl"
