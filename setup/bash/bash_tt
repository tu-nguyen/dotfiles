}

function gtt() {
  printf "${BOLD}${H1}git cheatsheet${NC}\n"
  printf "%s\n" "$br"
  printf "${H2}git${NC}                    → ${NC}ga${NC} (add)                     → ${NC}gb${NC} (branch)\n"
  printf "                       → ${NC}gs${NC} (status)                  → ${NC}gbr${NC} (fance branch)\n"
  printf "                       → ${NC}gc${NC} (cam)                     → ${NC}gbd${NC} (delete branch)\n"
  printf "                       → ${NC}cl${NC} (log)                     → ${NC}gcl${NC} (cl show last commit)\n"
  printf "                       → ${NC}crr${NC} (hard reset)             → ${NC}gll${NC} (bigger log)\n"
  printf "                       → ${NC}gf${NC} (git fetch)               → ${NC}gfetch${NC} (also git fetch)\n"
  printf "${H2}git alt${NC}                → ${NC}s${NC} (status -s)                → ${NC}b${NC} (fancy branch)\n"
  printf "                       → ${NC}st${NC} (status)                  → ${NC}br${NC} (branch)\n"
  printf "                       → ${NC}co${NC} (checkout)                → ${NC}a${NC} (add)\n"
  printf "                       → ${NC}cob${NC} (checkout -b)            → ${NC}ap${NC} (add -p)\ \n"
  printf "                       → ${NC}coB${NC} (checkout -B)            → ${NC}l${NC} (diff)\n"
  printf "                       → ${NC}c${NC} (commit)                   → ${NC}d${NC} (diff)\n"
  printf "                       → ${NC}ca${NC} (commit -a)               → ${NC}ds${NC} (diff --stat)\n"
  printf "                       → ${NC}cm${NC} (commit -m)               → ${NC}dc${NC} (diff --cached)\n"
  printf "                       → ${NC}cam${NC} (commit -a -m)           \n"
  printf "                       → ${NC}m${NC} (commit -m)                → ${H2}la${NC} (to ALL git config)\n"
  printf "%s\n" "$br"
  printf "${H2}git functions${NC}      \n"
  printf "  ${NC}gpull${NC}                → Git pull with rebase and autostash\n"
  printf "  ${NC}gpush${NC}                → Git push with set-upstream\n"
  printf "  ${NC}gco${NC}                  → Switch to a specified Git branch with confirmation\n"
  printf "  ${NC}gcob${NC}                 → Same as above but with fallback to create a branch\n"
  printf "  ${NC}greset${NC}               → Hard reset on current branch\n"
  printf "  ${NC}gclean${NC}               → Hard reset and removes untracked files\n"
  printf "  ${NC}gclone${NC}               → Clone a repo\n"
  printf "  ${NC}grn${NC}                  → Rename git branch\n"
  printf "%s\n" "$br"


  printf "${H2}ghcli run${NC}\n"
  printf "  ${NC}gh run list${NC}     → List runners (all details)\n"
  printf "  ${NC}gh run cancel${NC} ${NC}<run-id>${NC}   → Cancel a workflow run\n"
  printf "  ${NC}gh run list --json databaseId \n"
  printf "    -q '.[].databaseId' | "
  printf "    xargs -d '\n' -n1 \n"
  printf "    gh run cancel${NC}      → Cancels all runners\n"

  printf "  ${NC}gh run delete${NC}        → Delete a workflow run\n"
  printf "  ${NC}gh run rerun${NC}       → Rerun a workflow run\n"
  printf "  ${NC}gh run view${NC}     → View a workflow run\n"
  printf "  ${NC}gh run watch${NC}     → Watch a workflow run until it completes\n"
  printf "%s\n" "$br"
  printf "${H2}todo:${NC}\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "%s\n" "$br"
  printf "${H2}todo:${NC}\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "%s\n" "$br"
  printf "${H2}todo:${NC}\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "  ${H}todo${NC}                 → todo\n"
  printf "%s\n" "$br"
}


